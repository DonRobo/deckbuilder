<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<fx:root prefHeight="500.0" prefWidth="500.0" styleClass="cardStack" stylesheets="@card_stack.css" type="AnchorPane"
         xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="lblTitle" styleClass="stackLabel" text="Label" AnchorPane.leftAnchor="0.0"
               AnchorPane.topAnchor="0.0"/>
        <ScrollPane fx:id="spCards" hbarPolicy="NEVER" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
                    AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="25.0">
            <content>
                <FlowPane fx:id="fpCards"/>
            </content>
        </ScrollPane>
    </children>
</fx:root>
